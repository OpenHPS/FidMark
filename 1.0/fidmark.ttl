                            dcterms:title "Fiducial Marker Ontology"@en ;
                            <http://purl.org/vocab/vann/preferredNamespacePrefix> "fidmark" ;
                            <http://purl.org/vocab/vann/preferredNamespaceUri> "http://purl.org/fidmark/"^^xsd:anyURI ;
                            <http://www.w3.org/2004/02/skos/core#example> "https://raw.githubusercontent.com/OpenHPS/FidMark/main/examples/virtual_object.ttl"^^xsd:anyURI .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/fidmark/color
:color rdf:type owl:AnnotationProperty ;
       rdfs:comment "Describes the color of the marker."@en ;
       rdfs:label "marker color"@en ;
       rdfs:domain :FiducialMarker .


###  http://purl.org/fidmark/encoding
:encoding rdf:type owl:AnnotationProperty ;
          rdfs:comment "Describes the encoding method of the marker."@en ;
          rdfs:label "marker encoding"@en ;
          rdfs:domain :FiducialMarker .


###  http://purl.org/fidmark/opencvName
:opencvName rdf:type owl:AnnotationProperty ;
            rdfs:comment "OpenCV variable/object name"@en ;
            rdfs:label "OpenCV name"@en .


###  http://purl.org/fidmark/shape
:shape rdf:type owl:AnnotationProperty ;
       rdfs:comment "Describes the shape of the marker."@en ;
       rdfs:label "marker shape"@en ;
       rdfs:domain :FiducialMarker .


###  http://purl.org/fidmark/supportedMarker
:supportedMarker rdf:type owl:AnnotationProperty ;
                 rdfs:comment "Inidicates the relationship of a dictionary to a marker type."@en ;
                 rdfs:label "supported marker"@en ;
                 rdfs:range :FiducialMarker ;
                 rdfs:domain :MarkerDictionary .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/fidmark/hasDictionary
:hasDictionary rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://www.w3.org/ns/ssn/hasProperty> ;
               rdfs:domain :FiducialMarker ;
               rdfs:range :MarkerDictionary ;
               rdfs:comment "A marker dictionary defines the different variants that can be achieved using the available encoded bits and error correction."@en ;
               rdfs:label "has marker dictionary"@en .


###  http://purl.org/fidmark/hasHeight
:hasHeight rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf <http://www.w3.org/ns/ssn/hasProperty> ;
           rdfs:domain :FiducialMarker ;
           rdfs:comment "Defines the marker height in a specified unit. This can be used for determining the scale of the marker."@en ;
           rdfs:label "has marker height"@en .


###  http://purl.org/fidmark/hasImageDesciptor
:hasImageDesciptor rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf <http://www.w3.org/ns/ssn/hasProperty> ;
                   rdf:type owl:FunctionalProperty ,
                            owl:InverseFunctionalProperty ;
                   rdfs:domain :FiducialMarker ;
                   rdfs:range :ImageDescriptor ;
                   rdfs:label "has image descriptor"@en .


###  http://purl.org/fidmark/hasMarker
:hasMarker rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
           rdfs:range :FiducialMarker ;
           rdfs:comment "Indicates a marker on an (optical) positioning system"@en ;
           rdfs:label "has marker"@en .


###  http://purl.org/fidmark/hasOrigin
:hasOrigin rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf <http://www.w3.org/ns/ssn/hasProperty> ;
           rdfs:label "has origin"@en .


###  http://purl.org/fidmark/hasWidth
:hasWidth rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://www.w3.org/ns/ssn/hasProperty> ;
          rdfs:domain :FiducialMarker ;
          rdfs:comment "Defines the marker width in a specified unit. This can be used for determining the scale of the marker."@en ;
          rdfs:label "has marker width"@en .


###  http://purl.org/fidmark/isImageDescriptorOf
:isImageDescriptorOf rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf <http://www.w3.org/ns/ssn/isPropertyOf> ;
                     rdf:type owl:FunctionalProperty ,
                              owl:InverseFunctionalProperty ;
                     rdfs:label "is image descriptor of"@en .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/fidmark/asImage
:asImage rdf:type owl:DatatypeProperty ;
         rdfs:domain :ImageDescriptor ;
         rdfs:range xsd:anyURI ,
                    xsd:hexBinary ;
         rdfs:comment "A marker image links to an image URI or base64 representation of the marker."@en ;
         rdfs:label "as image"@en .


###  http://purl.org/fidmark/asNFT
:asNFT rdf:type owl:DatatypeProperty ;
       rdfs:domain :ImageDescriptor ;
       rdfs:range xsd:anyURI ,
                  xsd:hexBinary ;
       rdfs:label "as NFT"@en .


###  http://purl.org/fidmark/asZFT
:asZFT rdf:type owl:DatatypeProperty ;
       rdfs:domain :ImageDescriptor ;
       rdfs:range xsd:anyURI ,
                  xsd:hexBinary ;
       rdfs:label "as ZFT"@en .


###  http://purl.org/fidmark/crowns
:crowns rdf:type owl:DatatypeProperty ;
        rdfs:domain :CircularBarcode ;
        rdfs:range xsd:integer ;
        rdfs:comment "Indicates the number of crowns in a circular barcode."@en ;
        rdfs:label "number of crowns"@en .


###  http://purl.org/fidmark/dictionarySize
:dictionarySize rdf:type owl:DatatypeProperty ;
                owl:equivalentProperty :maxIdentifiers ;
                rdfs:domain :MarkerDictionary ;
                rdfs:label "dictionary size"@en .


###  http://purl.org/fidmark/hammingDistance
:hammingDistance rdf:type owl:DatatypeProperty ;
                 rdfs:domain :MarkerDictionary ;
                 rdfs:range xsd:integer ;
                 rdfs:comment "The minimum hamming distance between two codes"@en ;
                 rdfs:label "hamming distance"@en ;
                 <http://www.w3.org/2004/02/skos/core#altLabel> "tau"@en ;
                 <http://www.w3.org/2004/02/skos/core#example> "In a 16h5 dictionary, the first number indicates the amount of bits that can be encoded, the second number the minimum hamming distance."@en .


###  http://purl.org/fidmark/markerBits
:markerBits rdf:type owl:DatatypeProperty ;
            rdfs:domain :MarkerDictionary ;
            rdfs:range xsd:integer ;
            rdfs:comment "Marker bits indicates the amount of bits that are available."@en ;
            rdfs:label "marker bits"@en ;
            <http://www.w3.org/2004/02/skos/core#example> "In a 16h5 dictionary, the first number indicates the amount of bits that can be encoded, the second number the minimum hamming distance."@en .


###  http://purl.org/fidmark/markerCode
:markerCode rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:hexBinary ;
            rdfs:comment "Marker hexadecimal code"@en ;
            rdfs:label "marker code"@en .


###  http://purl.org/fidmark/markerData
:markerData rdf:type owl:DatatypeProperty ;
            rdfs:domain :FiducialMarker ;
            rdfs:range xsd:hexBinary ,
                       xsd:string ;
            rdfs:comment "Encoded information within a marker."@en ;
            rdfs:label "marker data"@en .


###  http://purl.org/fidmark/markerIdentifier
:markerIdentifier rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :markerData ;
                  rdfs:domain :FiducialMarker ;
                  rdfs:comment "If the marker can be identified using a specified dictionary, its identifier can be indicated using this data property."@en ;
                  rdfs:label "marker identifier"@en .


###  http://purl.org/fidmark/maxIdentifiers
:maxIdentifiers rdf:type owl:DatatypeProperty ;
                rdfs:domain :MarkerDictionary ;
                rdfs:range xsd:integer ;
                rdfs:label "maximum identifiers"@en .


###  http://purl.org/fidmark/version
:version rdf:type owl:DatatypeProperty ;
         rdfs:domain :MarkerDictionary ;
         rdfs:range xsd:integer ;
         rdfs:label "version"@en .


#################################################################
#    Classes
#################################################################

###  http://purl.org/fidmark/ARTag
:ARTag rdf:type owl:Class ;
       rdfs:subClassOf :Barcode2D ;
       :color "Monochrome"@en ;
       :encoding "Topological"@en ;
       :shape "Square"@en ;
       rdfs:comment "An ARTag is a fiducial marker system to support 3D registration (alignment) and pose tracking in augmented reality. They can be used to facilitate the appearance of virtual objects, games, and animations within the real world. Like the earlier ARToolKit system, they allow for video tracking capabilities that calculate a camera's position and orientation relative to physical markers in real time. Once the camera's position is known, a virtual camera can be positioned at the same point, revealing the virtual object at the location of the ARTag. It thus addresses two of the key problems in Augmented Reality: viewpoint tracking and virtual object interaction. An ARTag appears on the Mars Science Laboratory.A similar technique is being used by NASA's Spacecraft 3D smartphone app as an educational outreach tool. ARTag is supported by the open source Goblin XNA software."@en ;
       rdfs:isDefinedBy <https://dbpedia.org/resource/ARTag> ;
       rdfs:label "ARTag"@en .


###  http://purl.org/fidmark/ARToolKit
:ARToolKit rdf:type owl:Class ;
           rdfs:subClassOf :Barcode ;
           rdfs:label "ARToolKit"@en .


###  http://purl.org/fidmark/AprilTag
:AprilTag rdf:type owl:Class ;
          rdfs:subClassOf :Barcode2D ;
          :color "Monochrome"@en ;
          :encoding "Topological"@en ;
          :shape "Square"@en ;
          rdfs:comment "AprilTag is a visual fiducial system, useful for a wide variety of tasks including augmented reality, robotics, and camera calibration. Targets can be created from an ordinary printer, and the AprilTag detection software computes the precise 3D position, orientation, and identity of the tags relative to the camera. The AprilTag library is implemented in C with no external dependencies. It is designed to be easily included in other applications, as well as be portable to embedded devices. Real-time performance can be achieved even on cell-phone grade processors."@en ;
          rdfs:label "AprilTag"@en ;
          rdfs:seeAlso "https://april.eecs.umich.edu/software/apriltag"^^xsd:anyURI .


###  http://purl.org/fidmark/ArUco
:ArUco rdf:type owl:Class ;
       rdfs:subClassOf :Barcode2D ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasDictionary ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :MarkerDictionary
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :markerIdentifier ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:integer
                       ] ;
       :color "Monochrome"@en ;
       :encoding "Topological"@en ;
       :shape "Square"@en ;
       rdfs:comment "An ArUco marker is a synthetic square marker composed by a wide black border and an inner binary matrix which determines its identifier (id). The black border facilitates its fast detection in the image and the binary codification allows its identification and the application of error detection and correction techniques. The marker size determines the size of the internal matrix. For instance a marker size of 4x4 is composed by 16 bits."@en ;
       rdfs:isDefinedBy "https://docs.opencv.org/4.x/d5/dae/tutorial_aruco_detection.html"^^xsd:anyURI ;
       rdfs:label "ArUco"@en .


###  http://purl.org/fidmark/Barcode
:Barcode rdf:type owl:Class ;
         owl:equivalentClass <http://purl.org/iot/vocab/m3-lite#Barcode> ,
                             <http://schema.org/Barcode> ;
         rdfs:subClassOf :FiducialMarker ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :markerData ;
                           owl:minCardinality "1"^^xsd:nonNegativeInteger
                         ] ;
         rdfs:comment "A barcode or bar code is a method of representing data in a visual, machine-readable form."@en ;
         rdfs:isDefinedBy <https://dbpedia.org/resource/Barcode> ;
         rdfs:label "Barcode"@en .


###  http://purl.org/fidmark/Barcode1D
:Barcode1D rdf:type owl:Class ;
           rdfs:subClassOf :Barcode ;
           rdfs:comment "A barcode or bar code is a method of representing data in a visual, machine-readable form. Initially, barcodes represented data by varying the widths, spacings and sizes of parallel lines. These barcodes, now commonly referred to as linear or one-dimensional (1D), can be scanned by special optical scanners, called barcode readers, of which there are several types."@en ;
           rdfs:isDefinedBy <https://dbpedia.org/resource/Barcode> ;
           rdfs:label "1D barcode"@en .


###  http://purl.org/fidmark/Barcode2D
:Barcode2D rdf:type owl:Class ;
           rdfs:subClassOf :Barcode ;
           rdfs:comment "Two-dimensional (2D) use rectangles, dots, hexagons and other patterns, called matrix codes or 2D barcodes, although they do not use bars as such. 2D barcodes can be read using purpose-built 2D optical scanners, which exist in a few different forms. 2D barcodes can also be read by a digital camera connected to a microcomputer running software that takes a photographic image of the barcode and analyzes the image to deconstruct and decode the 2D barcode. A mobile device with an inbuilt camera, such as smartphone, can function as the latter type of 2D barcode reader using specialized application software (The same sort of mobile device could also read 1D barcodes, depending on the application software)."@en ;
           rdfs:isDefinedBy <https://dbpedia.org/resource/Barcode> ;
           rdfs:label "2D barcode"@en .


###  http://purl.org/fidmark/BinaryID
:BinaryID rdf:type owl:Class ;
          rdfs:subClassOf :Barcode ;
          rdfs:label "BinaryID"@en .


###  http://purl.org/fidmark/BullsEye
:BullsEye rdf:type owl:Class ;
          rdfs:subClassOf :Barcode ;
          rdfs:label "BullsEye"@en .


###  http://purl.org/fidmark/CALTag
:CALTag rdf:type owl:Class ;
        rdfs:subClassOf :Barcode2D ;
        rdfs:comment """CALTag is a self-identifying marker pattern that can be accurately and automatically detected in images. Detection is robust to occlusions, uneven illumination and moderate lens distortion.

Chequerboards are often used for camera calibration, with the interior saddle points providing the necessary point correspondences. Manual identification of these points is at best tedious, and at worst, infeasible and unreliable (especially when calibrating large arrays of cameras). By augmenting the pattern with self-identifying binary codes, much like the excellent ARTag system, this process can be automated.

Whereas ARTag has highly robust error detection and correction, it suffers from licence restrictions, and a somewhat inaccurate corner detector. CALTag employs rudimentary error detection, but the code is free to use and modify, and it detects corners using a very accurate saddle-point finder."""@en ;
        rdfs:isDefinedBy "https://www.cs.ubc.ca/labs/imager/tr/2010/Atcheson_VMV2010_CALTag/"^^xsd:anyURI ;
        rdfs:label "CALTag"@en ;
        <http://www.w3.org/2004/02/skos/core#altLabel> "Calibration tags"@en .


###  http://purl.org/fidmark/CCC
:CCC rdf:type owl:Class ;
     rdfs:subClassOf :Barcode1D ,
                     :CircularBarcode ;
     rdfs:label "Concentric contrasting circles"@en ;
     <http://www.w3.org/2004/02/skos/core#altLabel> "CCC"@en .


###  http://purl.org/fidmark/CCTag
:CCTag rdf:type owl:Class ;
       rdfs:subClassOf :CircularBarcode ;
       rdfs:comment "CCTag markers are a robust, highly accurate fiducial system that can be robustly localized in the image even under challenging conditions. The library can efficiently detect the position of the image of the (common) circle center and identify the marker based on the different ratio of their crown sizes. Each marker can be uniquely identified thanks to the thickness of each crown, which encodes the information of the marker, typically a unique ID."@en ;
       rdfs:isDefinedBy "https://cctag.readthedocs.io/en/latest/"^^xsd:anyURI ;
       rdfs:label "CCTag"@en .


###  http://purl.org/fidmark/Chilitags
:Chilitags rdf:type owl:Class ;
           rdfs:subClassOf :Barcode2D ;
           rdfs:comment "Chilitags are a C++ cross-platform software library for the detection and identification of 2D fiducial markers (a.k.a. tags). Their purpose is to be printed and placed on objects from the real world, to form the basis of Robotics or Augmented Reality (AR) applications. Using a camera, the position of the tagged objects can be acquired by a computer."@en ;
           rdfs:isDefinedBy "https://github.com/chili-epfl/chilitags"^^xsd:anyURI ;
           rdfs:label "Chilitags"@en .


###  http://purl.org/fidmark/ChromaTag
:ChromaTag rdf:type owl:Class ;
           rdfs:subClassOf :Barcode2D ;
           :color "Multicolor"@en ;
           rdfs:comment "ChromaTag is a colored fiducial marker and detection algorithm that is significantly faster than other markers."@en ;
           rdfs:isDefinedBy "https://github.com/CogChameleon/ChromaTag"^^xsd:anyURI ;
           rdfs:label "ChromaTag"@en .


###  http://purl.org/fidmark/CircularBarcode
:CircularBarcode rdf:type owl:Class ;
                 rdfs:subClassOf :Barcode ;
                 rdfs:comment "A circular barcode is made up of a series of concentric circles and is typically based on a standard barcode symbology. The barcode is thus readable by the same devices used to read traditional barcodes. The circular barcode can be scanned from any angle. The reading is not influenced by the orientation of the symbol. The redundancy in information ensures a good reading in every case, even if the surface is partially damaged."@en ;
                 rdfs:isDefinedBy "https://www.technoriversoft.com/CircularBarcode.html"^^xsd:anyURI ;
                 rdfs:label "Circular barcode"@en .


###  http://purl.org/fidmark/Cybercode
:Cybercode rdf:type owl:Class ;
           rdfs:subClassOf :Barcode2D .


###  http://purl.org/fidmark/FiducialMarker
:FiducialMarker rdf:type owl:Class ;
